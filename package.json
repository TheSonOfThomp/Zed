    "gulp-concat": "^2.6.1",
    "gulp-minify": "^3.1.0",
    "gulp-sass": "^4.1.0",
    "gulp-typescript": "^5.0.1",
    "nodemon": "^1.19.4",
    "react": "^16.13.1",
